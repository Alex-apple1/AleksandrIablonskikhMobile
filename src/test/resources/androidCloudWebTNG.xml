<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EPAM Android mobile cloud Web testing Suite">
    <parameter name="cloud" value="true"/>
    <parameter name="appType" value="web"/>
    <parameter name="deviceName" value="Samsung Galaxy S8"/>
    <!--  <parameter name="udid" value="ce11171b791f6c0603"/>-->
    <parameter name="platformName" value="Android"/>
    <parameter name="browserName" value="Chrome"/>
    <test name="Android Cloud Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>